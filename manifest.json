{
  "manifest_version": 3,
  "name": "Bookmark Manager by Aamu.app",
  "description": "Chronological bookmarks with folders as single-use tags.",
  "version": "1.0",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "action": {
    "default_title": "Open Bookmark Manager"
  },

  "permissions": [
    "bookmarks"
  ],

  "background": {
    "service_worker": "background.js"
  }
}